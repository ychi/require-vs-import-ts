{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
        "rootDir": "src",
        "outDir": "build",
      // Object.fromEntries
      "lib": ["dom", "es2017", "es2019.object"],
      "declaration": true,
      "composite": true,
      "emitDeclarationOnly": true,
      "isolatedModules": true,
      "importsNotUsedAsValues": "error",
  
      "strict": true,

      "moduleResolution": "node",
      "esModuleInterop": false,
      "resolveJsonModule": true
    },
  }